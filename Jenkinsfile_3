pipeline {
        agent

 {
        node {
            label  'temp'
            customWorkspace '/var/lib/jenkins/workspace/first_custom_workspce'
        }
 }
        stages {
        stage('Example') {
            steps {
                sh 'echo $WORKSPACE'
                checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: '290205a2-d519-45ae-90d3-0e23209e2c29', url: 'https://github.com/rajugajjela/git1.git']])
            }
        }
        stage('test') {
            steps {
                sh 'echo "hello"'
                script {
                    if (currentBuild.changeSets.size() > 0) {
                        echo 'Found ' + currentBuild.changeSets.size() + ' change sets'
                        def changeSets = currentBuild.changeSets

                    // Print information about each changeset
                        changeSets.each { changeSet ->
                                echo "Changeset in ${changeSet.kind} from ${changeSet.committer.displayName}:"
                                changeSet.items.each { item ->
                                            echo "  ${item.msg}"
                    }
                    else {
                        echo 'Found no change sets'
                    }
                }       
            }
        }
        }
}
